<div class='artist'>

	{{#if this.images}}
		<img src='{{this.images.0.url}}' alt='artist image' />
	{{else}}
		<img
			src='https://st3.depositphotos.com/23594922/31822/v/600/depositphotos_318221368-stock-illustration-missing-picture-page-for-website.jpg'
			alt='artist image'
		/>
	{{/if}}

	<div class='grey-bar'>
		<h3>{{this.name}}</h3>
		<button class='search-button' id='to-albums'><a href='/albums/{{this.id}}'>Albums</a></button>
	</div>
</div>